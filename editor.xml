<application>
  <component name="CodeInsightSettings">
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="false" />
        <entry key="FTL" value="false" />
        <entry key="Groovy" value="false" />
        <entry key="HTML" value="false" />
        <entry key="Haml" value="false" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JSP" value="false" />
        <entry key="JSPX" value="false" />
        <entry key="JavaScript" value="false" />
        <entry key="LESS" value="false" />
        <entry key="SASS" value="false" />
        <entry key="SCSS" value="false" />
        <entry key="SQL" value="false" />
        <entry key="Stylus" value="false" />
        <entry key="TypeScript" value="false" />
        <entry key="VTL" value="false" />
        <entry key="XHTML" value="false" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="false" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_DND_ENABLED" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern case-sensitive="true" pattern="\bfixme\b.*" />
    <pattern pattern="\bnotice\b.*" />
    <pattern pattern="\needCheck\b.*" />
    <pattern case-sensitive="true" pattern="\n* -tag: .*" />
  </component>
</application>